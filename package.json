{
  "name": "lit-it",
  "displayName": "lit-it",
  "description": "lazy JSdoc builder, add character to your code",
  "version": "1.2.12",
  "publisher": "mohseenrm",
  "engines": {
    "vscode": "^1.6.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mohseenrm/lit-it/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mohseenrm/lit-it.git"
  },
  "keywords": [
    "JS",
    "Formatters",
    "DOCStrings"
  ],
  "categories": [
    "Formatters",
    "Other"
  ],
  "icon": "images/lit-it.png",
  "galleryBanner": {
    "color": "#191919",
    "theme": "dark"
  },
  "activationEvents": [
    "onCommand:extension.litIt"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.litIt",
        "title": "Lit it!",
        "description": "Add JSdocs for your JS code"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node \"node_modules/vscode/bin/install\"",
    "pretest": "yarn tslint",
    "test": "node \"node_modules/vscode/bin/test\"",
    "tslint": "\"node_modules/.bin/tslint\" -c ./tslint.json \"./**/*.ts\" -e \"./node_modules/**\"",
    "tslint:quick-fix": "\"node_modules/.bin/tslint\" -c ./tslint.json \"./**/*.ts\" -e \"./node_modules/**\" --fix"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^12.6.1",
    "chai": "^4.2.0",
    "mocha": "^2.3.3",
    "tslint": "^5.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.2",
    "vscode": "^1.1.0"
  },
  "dependencies": {
    "global": "^4.4.0"
  }
}
